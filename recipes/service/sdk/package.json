{
  "name": "@sderickson/recipes-sdk",
  "description": "Tanstack queries and shared components for recipes service",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./fakes": "./fakes.ts",
    "./strings": "./strings.ts"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc",
    "dev": "NODE_OPTIONS='--experimental-strip-types --disable-warning=ExperimentalWarning' vite",
    "dev-docker": "npm exec saf-docker generate && docker compose up --build"
  },
  "devDependencies": {
    "@saflib/vitest": "*",
    "@testing-library/vue": "^8.1.0"
  },
  "dependencies": {
    "@saflib/monorepo": "*",
    "@saflib/sdk": "*",
    "@sderickson/recipes-spec": "*",
    "@saflib/vite": "*",
    "@saflib/vue": "*",
    "msw": "^2.11.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
